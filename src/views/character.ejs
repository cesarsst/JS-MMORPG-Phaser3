<html>
<head>

    <!-- Template styles-->
    <link rel="stylesheet" href="css/style.css">
    <link rel="stylesheet" href="css/bootstrap.min.css">
    
    <!-- Js Config -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="js/configs/login.js"></script>

    <!-- JS assets-->

    <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
    <script src="/socket.io/socket.io.js"></script>
    <script type="text/javascript" src="//cdn.jsdelivr.net/npm/phaser@3.21.0/dist/phaser.min.js"></script>
    <script type="text/javascript" src="js/characterScene/preload.js"></script>
    <script type="text/javascript" src="js/characterScene/game.js"></script>
